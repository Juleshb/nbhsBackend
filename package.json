{
  "name": "datacollectionbackend",
  "version": "1.0.0",
  "description": "Datacollection",
  "main": "index.js",
  "scripts": {
    "crateseeder": "npx sequelize-cli seed:generate --name demo-Users",
    "executeseeder": "npx sequelize-cli db:seed:all",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build/src/ ./src/ -s",
    "build": "npm run build-babel",
    "start-And-migration": "npm run migrate && npm start",
    "start-undomigrate": "npm run undomigrate && npm start",
    "start": "npx sequelize-cli db:seed:all && nodemon --exec babel-node ./src/index.js",
    "undomigrate": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "createmodel": "npx sequelize-cli model:generate --name Drafts --attributes motherName:string,fatherName:string,maritalStatus:string,phoneContact:string,province:string"
  },
  "author": "Alexis",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.41.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime": "^7.23.6",
    "sequelize-cli": "^6.6.2"
  }
}
